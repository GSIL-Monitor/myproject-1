<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8" />
<title data-lang="cn">重置密码-扫地机</title>
<meta id="viewport" name="viewport" content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0" />
<link rel="stylesheet" href="/css/foundation/normalize.min.css" />
<link rel="stylesheet" href="/css/foundation/foundation.min.css" />
<body class="antialiased hide-extras">
<style type="text/css">
body{background:#ededed;}
form,fieldset{border:none;margin:0;}
.errorMsg{color:#f00;position:relative;top:-.975em;margin-bottom:0;}
span.error{font-style:inherit;display:inline;}
.success{background:#43ac6a;color:#fff;font-size:0.75rem;padding:0.375rem 0.5625rem 0.5625rem;}
.nav{position:relative;z-index:1;overflow:hidden;display:block;text-align:center;background:#f9f9f9;-webkit-box-shadow:0 1px 1px rgba(0,0,0,0.2);-moz-box-shadow:0 1px 1px rgba(0,0,0,0.2);box-shadow:0 1px 1px rgba(0,0,0,0.2);}
.nav .tit{overflow:hidden;white-space:nowrap;text-overflow:ellipsis;display:inline-block;vertical-align:middle;max-width:75%;line-height:44px;font-size:1.125rem;color:#333;}
.nav .btn{float:right;margin:0 0 0 -42px;}
.nav .back{float:left;margin:0 -42px 0 0;padding:12px 9px 13px;}
.nav .btn .icon{display:block;width:24px;height:19px;background:url(data:image/png;base64,) no-repeat 0 0;-webkit-background-size:24px auto;background-size:24px auto;}
button:hover,button:focus,.button:hover,.button:focus,.button:active{background-color:#c8003d;border-color:#c8003d;}
.lang{color: #666; font-size: 0.75rem; line-height: 3; text-align: center;}
.tc{text-align: center;}
.su{color: #008cba;}
.er{color:#f00;}
.resetAfter{padding: 3.375rem 0;}
.hide{display: none;}
</style>
</head>
<body>
<div class="reset">
  <nav class="nav"> <a class="btn back" href="#"><i class="icon"></i></a> <span class="tit" data-lang="cn">重置密码</span> </nav>
  <div class="resetBox">
    <form action="/member/resetPassword" method="post" onsubmit="return submitFunc();" enctype="multipart/form-data">
      <fieldset>
        <legend class="hide" data-lang="cn">重置密码</legend>
        <div class="">
          <p class="errorMsg"><span id="Msg0" class="hide" data-lang="cn">链接异常，请从邮件中点击链接进行操作</span>&nbsp;</p>
        </div>
        <div class="">
          <label for="password" data-lang="cn">新设密码：</label>
          <input id="password" name="password" type="password" value="" placeholder="" />
          <p class="errorMsg"><span id="Msg1" class="hide" data-lang="cn">密码不能为空</span><span id="Msg11" class="hide" data-lang="cn">密码长度不小6位</span>&nbsp;</p>
        </div>
        <div class="">
          <label for="confirmPassword" data-lang="cn">确认密码：</label>
          <input id="confirmPassword" name="confirmPassword" type="password" value="" placeholder="" />
          <p class="errorMsg"><span id="Msg2" class="hide" data-lang="cn">两次密码输入不一致</span>&nbsp;</p>
        </div>
        <div class="">
          <input type="submit" class="postfix small button expand" value="提 交" data-lang="cn" /><span id="Msg3"></span>
          <p><a class="lang" href="#en" title="EngLish">EngLish</a></p>
        </div>
        <input id="token" name="token" type="hidden" value="{{token}}" />
        <input id="email" name="email" type="hidden" value="{{email}}" />
      </fieldset>
    </form>
  </div>
</div>
<div class="resetAfter tc hide">
    <p class="succ">
        <img src="data:image/png;base64," alt="success">        
    </p>
    <p class="err hide">
        <img src="data:image/png;base64," alt="error">
    </p>
    <strong id="tip"></strong>
</div>
<script src="/js/vendor/jquery.min.js"></script> 
<script>
var submitFunc = function() {
    var token = $("#token").val(),
        email = $("#email").val(),
        password = $("#password").val(),
        confirmPassword = $("#confirmPassword").val(),
        Msg0 = $('#Msg0'),
        Msg1 = $('#Msg1'),
        Msg11 = $('#Msg11'),
        Msg2 = $('#Msg2');
    var _yl = Msg0.attr('data-lang');
    if (!token || !email) {
        Msg0.show();
        return false;
    } else {
        Msg0.hide();
    }
    if (!password) {
        Msg1.show();
        return false;
    } else if (password.length< 6) {
        Msg1.hide();
        Msg11.show();
        return false;
    } else {
        Msg11.hide();
    }
    if (password != confirmPassword) {
        Msg2.show();
        return false;
    } else {
        Msg2.hide();
    }    
    $.ajax({
        url: '/member/'+_yl+'/resetPassword',
        type: 'post',
        data: {
            'token': token,
            'email': email,
            'password': password,
            'confirmPassword': confirmPassword
        },
        success: function(json) {
            var json = $.parseJSON(json),
            //_hash = location.hash,
            _text = null,
            _msg = $('#Msg3');   
   
            if (json.code == 'N00000') {
                _msg.removeClass('error').addClass('success');
                Msg0.hide();
                Msg1.hide();
                Msg11.hide();
                Msg2.hide();
                var tip = '<span class="su">'+json.message+'</span>';
                $('.succ').show();
                $('.err').hide();
            } else {            
                _msg.removeClass('success').addClass('error'); 
                var tip = '<span class="er">'+json.message+'</span>';    
                $('.succ').hide();
                $('.err').show();           
            }
            _text = (_yl=='en')?"TIPS:":"提示：";
            _msg.html(_text+json.message);
            $('#tip').html(tip);
            $('.resetBox').hide();
            $('.resetAfter').show();            
        }
    });
    return false;
};

$('.back').on('click',function(){
    window.history.go(-1); 
    return false;
    //window.open("#", "_self").close();
});
$(function() {
    function lang(lang) {
        var _locale = [{
                "cn": "重置密码-扫地机",
                "en": "Reset password"
            }, {
                "cn": "重置密码",
                "en": "Reset Password"
            }, {
                "cn": "重置密码",
                "en": "Reset Password"
            }, {
                "cn": "链接异常，请从邮件中点击链接进行操作",
                "en": "Warring,please click link from mail"
            }, {
                "cn": "新设密码",
                "en": "New Password"
            }, {
                "cn": "密码不能为空",
                "en": "Enter New Password"
            }, {
                "cn": "密码长度不小6位",
                "en": "Password's Lenght > 6"
            }, {
                "cn": "确认密码",
                "en": "Confirm Password"
            }, {
                "cn": "两次密码输入不一致",
                "en": "Confirm Password is not consistent"
            }, {
                "cn": "提 交",
                "en": "Confirm"
            }],
            _elm = $('[data-lang]'),
            _lang = (lang == undefined || !lang || lang == 'cn') ? 'cn' : 'en';

        _elm.each(function(i, elm) {
            var _this = $(this),
                _isLabel = _this.is('label')
            _isInput = _this.is('input'),
                _text = _locale[i][_lang];
            _this.attr('data-lang',_lang);
            if (_isLabel) {
                _this.next('input').attr({
                    'placeholder': _text
                });
            }
            if (_isInput) {
                _this.attr({
                    'placeholder': _text
                })
                if(_this.attr('type')=='submit'){
                    _this.val(_text);
                }
            } else {
                _this.text(_text);
            }
        });
    }
    var _hash = location.hash,_lang = 'cn';
    if(_hash&&_hash!=null&&_hash!=undefined&&_hash=='#en'){
        _lang = 'en';
    }
    lang(_lang);
    $(document).on('click','a.lang',function(){
        var _this = $(this),_lang = _this.attr('href');
        _lang = _lang.replace('#','');
        lang(_lang);
        if(_lang=='en'){
            _this.text('中国').attr('href','#cn');
        }else{
            _this.text('EngLish').attr('href','#en');
        }
    });
    //清除错误
    $('#password,#confirmPassword').on('focus',function(){
       var Msg0 = $('#Msg0'),
           Msg1 = $('#Msg1'),           
           Msg2 = $('#Msg2'),
           Msg3 = $('#Msg3'),
           Msg11 = $('#Msg11');

        Msg3.removeClass('error').removeClass('success');
        Msg0.hide();
        Msg1.hide();
        Msg11.hide();
        Msg2.hide();
    });
});
</script>
</body>
</html>