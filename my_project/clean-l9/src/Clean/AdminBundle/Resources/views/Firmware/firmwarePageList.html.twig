{% include 'CleanAdminBundle:Common:meta.html.twig' %}
<title>固件版本列表</title>
</head>
<body>
<nav class="breadcrumb"><i class="Hui-iconfont">&#xe67f;</i> 首页 <span class="c-gray en">&gt;</span> 固件版本管理 <span class="c-gray en">&gt;</span> 固件版本列表 <a class="btn btn-success btn-refresh radius r" style="line-height:1.6em;margin-top:3px" href="javascript:location.replace(location.href);" title="刷新" ><i class="Hui-iconfont">&#xe68f;</i></a></nav>
<div class="page-container">
    <div class="text-c">
        <span class="select-box inline"> 
                固件版本号     
            <select name="searchType" id="searchType" class="select">                   
              <option value="1">等于</option>
              <option value="2">大于</option>
            </select>
        </span>
        <input type="text" class="input-text" style="width:140px" placeholder="输入固件版本号" id="version" name="version">
        <button type="submit" class="btn btn-success" id="search" name="search"><i class="Hui-iconfont">&#xe665;</i> 搜索</button>
    </div>
    <div class="cl pd-5 bg-1 bk-gray mt-20"> <span class="l"><a href="javascript:;" id="delAllList" class="btn btn-danger radius"><i class="Hui-iconfont">&#xe6e2;</i> 批量删除</a> <a href="javascript:;" onclick="pageFullShow('添加固件版本','/admin/addFirmware')" class="btn btn-primary radius"><i class="Hui-iconfont">&#xe600;</i> 添加固件版本</a></span> <span class="r">共有数据：<strong id="listCount">0</strong> 条</span> </div>
    <table class="table table-border table-bordered table-bg" id="dataList">
        <thead>
            <tr>
                <th scope="col" colspan="10">固件版本列表</th>
            </tr>
            <tr class="text-c">
                <th width="25"><input type="checkbox" name="checkAll" value=""></th>
                <th>公司</th> 
                <th>固件名称</th>
                <th width="49">版本</th>
                <th>APP显示版本</th>
                <th>地址</th>
                <th>校验码</th>
                <th>更新时间</th>
                <th>创建时间</th>          
                <th width="100">操作</th>
            </tr>
        </thead>
        <tbody>

        </tbody>
    </table>
    <div id="page" class="mt-20"></div>
</div>

{% include 'CleanAdminBundle:Common:footer.html.twig' %}

<script type="text/javascript" src="/assets/lib/datatables/1.10.0/jquery.dataTables.min.js"></script> 
<script type="text/javascript" src="/assets/lib/laypage/1.2/laypage.js"></script>
<script type="text/javascript" src="/assets/js/firmwarePageList.min.js"></script>
</body>
</html>
