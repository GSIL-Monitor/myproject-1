{% include 'CleanAdminBundle:Common:meta.html.twig' %}
<title>固件白名单列表</title>
</head>
<body>
<nav class="breadcrumb"><i class="Hui-iconfont">&#xe67f;</i> 首页 <span class="c-gray en">&gt;</span> 固件白名单管理 <span class="c-gray en">&gt;</span> 固件白名单列表 <a class="btn btn-success btn-refresh radius r" style="line-height:1.6em;margin-top:3px" href="javascript:location.replace(location.href);" title="刷新" ><i class="Hui-iconfont">&#xe68f;</i></a></nav>
<div class="page-container"> 
    <div class="text-c">   
        <input type="text" class="input-text" style="width:350px" placeholder="或输入SN" id="sn" name="sn">
        <button type="submit" class="btn btn-success radius" id="search" name="search"><i class="Hui-iconfont">&#xe665;</i> 搜索</button>
    </div>
    <div class="uploader-thum-container" style="position: absolute; top: -1000px;">
        <div id="fileList" class="uploader-list"></div>
        <div id="filePicker">点击上传</div>
    </div>
    <div class="cl pd-5 bg-1 bk-gray mt-20"> <span class="l"><a href="javascript:;" id="delAllList" class="btn btn-danger radius"><i class="Hui-iconfont">&#xe6e2;</i> 批量删除</a> <a href="javascript:;" id="uploadWhiteGroupSnExcel" class="btn btn-success radius"><i class="Hui-iconfont">&#xe61d;</i> 导入名单</a> <a href="javascript:;" onclick="snsModeShow('添加固件白名单','/admin/addWhiteGroupSnSubmit','','')" class="btn btn-primary radius"><i class="Hui-iconfont">&#xe600;</i> 添加固件白名单</a></span> <span class="r">共有数据：<strong id="listCount">0</strong> 条</span> </div>
    <table class="table table-border table-bordered table-bg" id="dataList">
        <thead>
            <tr>
                <th scope="col" colspan="5">{{whiteGroupName|raw}}组白名单列表</th>
            </tr>
            <tr class="text-c">
                <th width="25"><input type="checkbox" name="checkAll" value=""></th>
                <th>SN</th> 
                <th>备注</th>
                <th>时间</th>           
                <th width="100">操作</th>
            </tr>
        </thead>
        <tbody>

        </tbody>
    </table>
    <div id="page" class="mt-20"></div>
</div>
<article class="page-container" id="addSns" style="display: none;">
    <form class="form form-horizontal" id="form" action="/admin/addWhiteGroupSnSubmit" method="post"> 
        <div class="row cl">
            <label class="form-label col-xs-4 col-sm-2">SN：</label>
            <div class="formControls col-xs-8 col-sm-9">             
               <input type="hidden" class="input-text" value="{{whiteGroupId}}" id="whiteGroupId" name="whiteGroupId">
               <input type="text" class="input-text" value="" placeholder="SN" id="sn1" name="sn1">
            </div>
        </div>
        <div class="row cl">
            <label class="form-label col-xs-4 col-sm-2">备注：</label>
            <div class="formControls col-xs-8 col-sm-9"> 
               <input type="text" class="input-text" name="noteName" id="noteName" placeholder="备注" value="">
            </div>
        </div>              
        <div class="row cl">
            <div class="col-xs-8 col-sm-9 col-xs-offset-4 col-sm-offset-2">
                <button class="btn btn-primary radius" id="submit" type="submit"><i class="Hui-iconfont">&#xe632;</i> 保存</button>                
                <button id="reset" class="btn btn-default radius" type="button">&nbsp;&nbsp;取消&nbsp;&nbsp;</button>
            </div>
        </div>
    </form>
</article>
{% include 'CleanAdminBundle:Common:footer.html.twig' %}

<script type="text/javascript" src="/assets/lib/jquery.validation/1.14.0/jquery.validate.js"></script> 
<script type="text/javascript" src="/assets/lib/jquery.validation/1.14.0/validate-methods.js"></script> 
<script type="text/javascript" src="/assets/lib/jquery.validation/1.14.0/messages_zh.js"></script> 
<script type="text/javascript" src="/assets/lib/webuploader/0.1.5/webuploader.min.js"></script>
<script type="text/javascript" src="/assets/lib/datatables/1.10.0/jquery.dataTables.min.js"></script> 
<script type="text/javascript" src="/assets/lib/laypage/1.2/laypage.js"></script>
<script type="text/javascript" src="/assets/js/whiteGroupSnPageList.min.js"></script>
</body>
</html>
